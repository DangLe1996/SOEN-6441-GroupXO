# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page


GET   /							controllers.HomeController.searchPage(request: Request)
POST  /                    controllers.HomeController.gettweet(request: Request)

GET   /user							controllers.HomeController.user(s: String)
GET   /location							controllers.HomeController.location(s: String)
GET   /word							controllers.HomeController.word(s: String)
GET  /hashtag                       controllers.HomeController.hashtag(request: Request,s: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
